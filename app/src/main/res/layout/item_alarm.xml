<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/iv_alarmviewitem_profile"
        android:layout_margin="7.5dp"
        android:layout_width="35dp"
        android:layout_height="35dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tv_alarmviewitem_profile"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <TextView
        android:id="@+id/tv_alarmviewitem_profile"
        android:textStyle="bold"
        android:text="Alarm Message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"
        app:layout_constraintTop_toTopOf="@id/iv_alarmviewitem_profile"
        app:layout_constraintBottom_toBottomOf="@id/iv_alarmviewitem_profile"
        app:layout_constraintStart_toEndOf="@id/iv_alarmviewitem_profile"/>
</androidx.constraintlayout.widget.ConstraintLayout>